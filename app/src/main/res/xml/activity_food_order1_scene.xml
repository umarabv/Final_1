<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <OnClick
            motion:targetId="@id/button2"
            motion:clickAction="toggle"/>
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="199dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView14"
            android:layout_width="373dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/imageView16"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView15"
            motion:layout_constraintVertical_bias="1.0"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"
            android:id="@+id/imageView15"
            motion:layout_constraintTop_toTopOf="@+id/editText3" />
        <Constraint
            android:layout_height="124dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="124dp"
            android:layout_width="330dp"
            motion:layout_constraintTop_toBottomOf="@+id/imageView15"
            android:id="@+id/imageView20" />
    </ConstraintSet>
</MotionScene>